
(rules PCB usbavrfloppy
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 50)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.4)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.7)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_600:400_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_600:300_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_600:400_um" "Via[0-1]_600:400_um" default
  )
  (via 
    "Via[0-1]_600:300_um" "Via[0-1]_600:300_um" default
  )
  (via 
    "Via[0-1]_600:400_um-kicad_default" "Via[0-1]_600:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_600:300_um-kicad_default" "Via[0-1]_600:300_um" "kicad_default"
  )
  (via 
    "Via[0-1]_600:400_um-+12V" "Via[0-1]_600:400_um" +12V
  )
  (via 
    "Via[0-1]_600:300_um-+12V" "Via[0-1]_600:300_um" +12V
  )
  (via 
    "Via[0-1]_600:400_um-GND" "Via[0-1]_600:400_um" GND
  )
  (via 
    "Via[0-1]_600:300_um-GND" "Via[0-1]_600:300_um" GND
  )
  (via 
    "Via[0-1]_600:400_um-VCC" "Via[0-1]_600:400_um" VCC
  )
  (via 
    "Via[0-1]_600:300_um-VCC" "Via[0-1]_600:300_um" VCC
  )
  (via 
    "Via[0-1]_600:400_um-VCC_USB" "Via[0-1]_600:400_um" "VCC_USB"
  )
  (via 
    "Via[0-1]_600:300_um-VCC_USB" "Via[0-1]_600:300_um" "VCC_USB"
  )
  (via_rule
    default "Via[0-1]_600:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_600:400_um-kicad_default"
  )
  (via_rule
    +12V "Via[0-1]_600:300_um-+12V"
  )
  (via_rule
    GND "Via[0-1]_600:300_um-GND"
  )
  (via_rule
    VCC "Via[0-1]_600:300_um-VCC"
  )
  (via_rule
    "VCC_USB" "Via[0-1]_600:300_um-VCC_USB"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(C1-Pad2)" "Net-(C2-Pad2)" "Net-(C4-Pad2)" /MISO /SCK /MOSI "Net-(D1-Pad1)" "Net-(J1-Pad2)"
    /~INDEX /~DIR /~STEP /~WDATA /~WGATE /~TRK0 /~RDATA "/USBD-"
    /USBD+ /CTS /RXD /TXD /~mRESET /~MOTEB /~WPROT /~SIDE1
    /~mSTEP /~mTRK0 /~mWGATE /~mINDEX /~mRDATA /~mDIR /~mWDATA /~mHSEL
    /~mMOTEN /~mWPROTECT "VCC_USB" "Net-(D2-Pad2)" "Net-(D3-Pad2)" VCC
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class +12V
    +12V "Vcc_FLOPPY"
    (clearance_class +12V)
    (via_rule +12V)
    (rule
      (width 350.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class GND
    GND
    (clearance_class GND)
    (via_rule GND)
    (rule
      (width 350.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class VCC
    (clearance_class VCC)
    (via_rule VCC)
    (rule
      (width 350.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "VCC_USB"
    (clearance_class "VCC_USB")
    (via_rule VCC_USB)
    (rule
      (width 350.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)